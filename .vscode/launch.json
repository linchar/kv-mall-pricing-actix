    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Launch",
            "program": "${workspaceFolder}/examples/client.rs",
            "args": [],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'pricing_rust'",
            "cargo": {
                "args": [
                    "build",
                    "--bin=pricing_rust",
                    "--package=pricing_rust"
                ],
                "filter": {
                    "name": "pricing_rust",
                    "kind": "bin"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug unit tests in executable 'pricing_rust'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--bin=pricing_rust",
                    "--package=pricing_rust"
                ],
                "filter": {
                    "name": "pricing_rust",
                    "kind": "bin"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug example 'client'",
            "cargo": {
                "args": [
                    "build",
                    "--example=client",
                    "--package=pricing_rust",
                    "--features=awc"
                ],
                "filter": {
                    "name": "client",
                    "kind": "example"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug unit tests in example 'client'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--example=client",
                    "--package=pricing_rust"
                ],
                "filter": {
                    "name": "client",
                    "kind": "example"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        }
    ]
}